<?xml version="1.0" encoding="utf-8"?>
<!--
  Declares which USB devices should launch this app when plugged in.
  VID/PID pairs for common ELM327 USB chips:
    0x0403/0x6001 = FTDI FT232R
    0x0403/0x6015 = FTDI FT230X
    0x067B/0x2303 = Prolific PL2303
    0x067B/0x23A3 = Prolific PL2303HXD
    0x1A86/0x7523 = QinHeng CH340
    0x1A86/0x5523 = QinHeng CH341
-->
<resources>
    <usb-device vendor-id="1027"  product-id="24577" /> <!-- 0x0403/0x6001 FTDI FT232R  -->
    <usb-device vendor-id="1027"  product-id="24597" /> <!-- 0x0403/0x6015 FTDI FT230X  -->
    <usb-device vendor-id="1659"  product-id="8963"  /> <!-- 0x067B/0x2303 Prolific PL2303 -->
    <usb-device vendor-id="1659"  product-id="9123"  /> <!-- 0x067B/0x23A3 Prolific HXD  -->
    <usb-device vendor-id="6790"  product-id="29987" /> <!-- 0x1A86/0x7523 CH340         -->
    <usb-device vendor-id="6790"  product-id="21795" /> <!-- 0x1A86/0x5523 CH341         -->
</resources>
